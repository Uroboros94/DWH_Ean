						"name": "num_order",
						"type": "varchar"
					},
					{
						"name": "OrderId",
						"type": "varchar"
					},
					{
						"name": "fec_orden",
						"type": "date"
					},
					{
						"name": "fec_envio",
						"type": "date"
					},
					{
						"name": "imp_subtotal",
						"type": "float",
						"precision": 15
					},
					{
						"name": "id_modo",
						"type": "int",
						"precision": 10
					},
					{
						"name": "modo_envio",
						"type": "varchar"
					},
					{
