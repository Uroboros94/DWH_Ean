			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/DimMetas')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ConnectorAzureRafael",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "DWH"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "PK_Meta",
						"type": "int",
						"precision": 10
					},
					{
						"name": "Año",
						"type": "int",
						"precision": 10
					},
					{
						"name": "Mes",
						"type": "int",
						"precision": 10
					},
					{
						"name": "Region_ID",
						"type": "int",
						"precision": 10
					},
					{
