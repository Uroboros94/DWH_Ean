{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DHWETL"
		},
		"BankMarketSources_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'BankMarketSources'"
		},
		"DWHGroup1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DWHGroup1'"
		},
		"bankMarketDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'bankMarketDatabase'"
		},
		"BankMarketSources_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakebankmarket.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DHWETL_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"BankMarketSources_accountKey": {
						"value": "[parameters('BankMarketSources_accountKey')]"
					},
					"DWHGroup1_connectionString": {
						"value": "[parameters('DWHGroup1_connectionString')]"
					},
					"bankMarketDatabase_connectionString": {
						"value": "[parameters('bankMarketDatabase_connectionString')]"
					},
					"BankMarketSources_properties_typeProperties_url": {
						"value": "[parameters('BankMarketSources_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}